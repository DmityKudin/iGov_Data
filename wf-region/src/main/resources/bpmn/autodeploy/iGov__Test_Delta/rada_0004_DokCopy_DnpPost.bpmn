    <scriptTask id="scripttask1" name="Script Task (javascript)" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var typeMail = execution.getVariable('asTypeMail');
var sum = 16.09;
var sumToScreen = "15.80";
var sumNote = "Загальна сума сплати становить 16.09 грн (до вартості послуги 15.80 грн. буде додано комісію 1,8%)";
if (typeMail === 'sRekPrior') {
	sum =  36.45;
	sumToScreen = "35.80";
	sumNote = "Загальна сума сплати становить 36.45 грн (до вартості послуги 35.80 грн. буде додано комісію 1,8%)";
}
execution.setVariable('sSum', sum);
execution.setVariable('sSumScreen', sumToScreen);
execution.setVariable('sSumNote', sumNote);

var delta='1';
var unixdate= Math.round((new Date()).getTime());
var unixdate=unixdate+86400*1000*delta;
var new_src = new Date(unixdate);
var d = new_src.getDate();
if (d &lt; 10) d = "0"+d;
var m = new_src.getMonth();
var m = (+m + +1);
if (m &lt; 10) m = "0"+m;

var new_year = new_src.getFullYear();
var new_hour = new_src.getHours();
if (new_hour &lt; 10) new_hour = "0" + new_hour;
var new_minutes = new_src.getMinutes();
if (new_minutes &lt; 10) new_minutes = "0" + new_minutes;
var timer = new_year+'-'+m+'-'+d+'T'+new_hour+':'+new_minutes+':00';


timer = 'P1DT5M';
execution.setVariable('sNotification_day', timer)
timer = '2016-12-16T01:05:00'
timer = 'PT5M';
timer = 'PT1H';
timer = 'P1DT5M';

var per1 = ' '
var per2 = ' '
if (execution.getVariable('sNumRoom_293') != null &amp;&amp; execution.getVariable('sNumRoom_293') != ' ')
{
	per1 = execution.getVariable('sAddress') + ', ' + execution.getVariable('sNumRoom_293')
	execution.setVariable('sAddress', per1)
}
		
if (execution.getVariable('asDocType') == 'sDocTypeGolova')
	per2= 'копію розпорядження міського голови'
else
	per2= 'копію рішення міської ради'

execution.setVariable('docTyp', per2)
		
</script>
    </scriptTask>
