    <scriptTask id="scripttask1" name="Script Task" scriptFormat="groovy" activiti:autoStoreVariables="true">
      <script>
/* Заполнение полей */
/* Заявник */
if(execution.getVariable('asWork0')=='no'){
	execution.setVariable('sWorkPlace0', 'не працює');
}
if(execution.getVariable('asPrivilege')=='no'){
	execution.setVariable('sPrivilege', 'не має');
}

// скільки проживає людей
def maxCountFamily = 6
def countFamily = Integer.parseInt(execution.getVariable('asCountFamily')[1])
def countIsAdult = 0

/*форматуємо поле місце роботи*/
for (int i = 1; i &lt;= maxCountFamily; i++) {
    def name_asWork = 'asWork' + "${i}"
    def name_sWorkPlace = 'sWorkPlace' + "${i}"  
    def name_asAdult = 'asAdult' + "${i}"      
    def name_sFamilyRelation = 'sFamilyRelation' + "${i}"  
    def name_nBirthday = 'nBirthday' + "${i}"  
    def name_nWhereLives = 'nWhereLives' + "${i}"
    def name_sFIO = 'sFIO' + "${i}"
    def name_sClFio = 'sClFio' + "${i}"
	if(i &lt;= countFamily){
		def ClFio = execution.getVariable(name_sFIO) + ', ' + execution.getVariable(name_sFamilyRelation) + ', ' + execution.getVariable(name_nBirthday) + ' р.н.'     
		execution.setVariable(name_sClFio, ClFio)     
		if(execution.getVariable(name_asAdult)== 'yes'){
			countIsAdult ++;
			if(execution.getVariable(name_asWork)== 'no'){
				execution.setVariable(name_sWorkPlace, 'не працює')    
			}
		}
		else{
			execution.setVariable(name_sWorkPlace, 'не працює')    
		}
	}
	if(i &gt; countFamily){/*родственники НЕ существуют*/     
		execution.setVariable(name_sWorkPlace, ' ')
		execution.setVariable(name_sFamilyRelation, ' ')   
		execution.setVariable(name_nBirthday, ' ')  
		execution.setVariable(name_nWhereLives, ' ')   
		execution.setVariable(name_sFIO, ' ')
		execution.setVariable(name_sClFio, '')
	}
}
execution.setVariable('nCountIsAdult', 'a' + countIsAdult)
		</script>
    </scriptTask>
