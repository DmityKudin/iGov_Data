        <activiti:formProperty id="sBody_2" name="[pattern/print/dnepr_cnap_274(2)_print.html]; ;bVisible=false" type="string" default="Заява для м.Боярка" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankId_scan_passport" name="Сканований паспорт громадянина; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="doc" name="Викопіювання з ситуаційного плану міста М 1:500 з зазначенням бажаного місця розташування тимчасової споруди погоджене з балансоутримувачами інженерних мереж міста; (ЦТП №12 ДФ ПАТ «Укртелеком», КП «Теплові мережі», ПрАТ «ПЕЕМ»ЦЕК», КП «Жилсервіс», місцева дільниця УЕГГ).; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
