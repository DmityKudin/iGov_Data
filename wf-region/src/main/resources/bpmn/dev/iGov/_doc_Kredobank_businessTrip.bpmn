    <scriptTask id="scripttask1" name="ProcessInstanceId/ПІБ заявника/посада/департамент/керівник" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>//1.Получение ИД текущего процесса
var processInstanceId = execution.getProcessInstanceId();
execution.setVariable('processInstanceId', processInstanceId)

//2. ПІБ автора
var sLoginAuthor = execution.getVariable('sLoginAuthor')
var oResultAuthorName = subjectService.getSubjectByLoginAccount(sLoginAuthor)
execution.setVariable('sNameAuthor', oResultAuthorName.sLabel)
// 3.Департамент автора
var obj2 = subjectGroupTreeService.getSubjectGroupsTreeUp(sLoginAuthor,'Organ')
var sDepartment_Author = obj2[0].oSubject.sLabel
execution.setVariable('sDepartment_Author', sDepartment_Author)
//4.Посада автора
var obj3 = subjectGroupService.getSubjectGroup(sLoginAuthor)
var sPosition_Author = obj3.oSubjectHumanPositionCustom.sNote
execution.setVariable('sPosition_Author', sPosition_Author)
// 5.Дані керівника заявника,ПІБ,логін,посада
var obj1 = subjectGroupTreeService.getSubjectGroupsTreeUp(sLoginAuthor,'Human')
var sLogin_Head = obj1[0].sID_Group_Activiti
var sName_Head = obj1[0].oSubject.sLabel
var sPosition_Head = obj1[0].oSubjectHumanPositionCustom.sNote

execution.setVariable('sLogin_Head', sLogin_Head)
execution.setVariable('sName_Head', sName_Head)
execution.setVariable('sPosition_Head', sPosition_Head)
//6. лічільник
var number=execution.getVariable('sID_Custom_GovPublic')
number = number&lt;&lt;0
var fullNumber=number+"-В/Д"
execution.setVariable('sID_Custom_GovPublic', fullNumber)</script>
    </scriptTask>
