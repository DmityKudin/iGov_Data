    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var isRegistered  = ""
var sMessage =""

if (execution.getVariable('asDecision') == 'sEnumDecisionReturn'){
  isRegistered = "не "
}

sMessage = "&lt;p&gt;Надіслане до Департаменту з питань державного архітектурно-будівельного контролю міста Києва виконавчого органу Київської міської ради (Київської міської державної адміністрації) повідомлення про початок виконання будівельних робіт "+ isRegistered + "зареєстровано у єдиному реєстрі документів, що дають право на виконання підготовчих та будівельних робіт і засвідчують прийняття в експлуатацію закінчених будівництвом об’єктів, відомостей про повернення на доопрацювання, відмову у видачі, скасування та анулювання зазначених документів.&lt;/p&gt;"

if (execution.getVariable('asDecision') == 'sEnumDecisionOK'){
  sMessage = sMessage + "&lt;p&gt;Реєстраційний номер: " + execution.getVariable('sID_NumberDABI')+"&lt;/p&gt;"
}
execution.setVariable('sTextLetter', sMessage)</script>
    </scriptTask>
